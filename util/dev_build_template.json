{
  "name": "dev-sneekpeek",
  "source": {
    "type": "CODECOMMIT",
    "location": "https://git-codecommit.us-east-1.amazonaws.com/v1/repos/sneakpeek"
  },
  "artifacts": {
    "type": "S3",
    "location": "codebuild-dev-sneakpeek"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/standard:2.0",
    "computeType": "BUILD_GENERAL1_SMALL"
  },
  "serviceRole": "arn:aws:iam::350255258796:role/DevSneakPeekCodeBuild",
  "badgeEnabled":true,
  "timeoutInMinutes":5,
  "logsConfig":{
      "cloudWatchLogs": {
        "status": "ENABLED",
        "groupName": "codebuild-dev-sneakpeek"
      },
    "s3Logs": {
        "status": "ENABLED",
        "location": "codebuild-dev-sneakpeek/buildlogs"
      }
  }


}
